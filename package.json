{
    "name": "proyecto-telas",
    "productName": "Pichoncitos Morteros",
    "version": "1.0.0",
    "description": "Programa para manejar telas",
    "main": "src/index.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . proyect_telas --platform=win32 --arch=ia32 --icon=assets/icons/win/pichoncitos.ico",
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/93Gabix/proyecto-telas.git"
    },
    "author": "GabrielMiguel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/93Gabix/proyecto-telas/issues"
    },
    "homepage": "https://github.com/93Gabix/proyecto-telas#readme",
    "dependencies": {
        "bootstrap-icons": "^1.0.0",
        "electron": "^10.1.2",
        "electron-packager": "^15.1.0",
        "electron-reload": "^1.5.0",
        "lowdb": "^1.0.0",
        "moment": "^2.29.0",
        "requirejs": "^2.3.6",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "electron-installer-windows": "^3.0.0"
    }
}
